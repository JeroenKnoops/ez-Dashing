    var style = {
      lineStyle: {
        stroke: "#FFF",
        strokeWidth: 4
      }
    };


    return (
      <svg height="100%" width="100%" viewBox="0 0 130 130">
        <line x1="10" y1="10" x2="100" y2="100" style={style.lineStyle} />
      </svg>
    );


    return (
      <svg viewBox="0 0 20 10">
        <polygon fill="red" stroke-width="5" points="0,0 10,10 20,0" />
      </svg>
    );
